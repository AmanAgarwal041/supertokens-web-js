{
    "name": "supertokens-web-js",
    "version": "0.0.1",
    "description": "SuperTokens SDK for vanilla JS for all recipes",
    "main": "./index.js",
    "scripts": {
        "build-check": "cd lib && npx tsc -p tsconfig.json --noEmit",
        "check-circular-dependencies": "npx madge --circular --extensions js ./lib/build/",
        "build": "cd lib && rm -rf build && npx tsc -p tsconfig.json",
        "pretty-check": "npx pretty-quick --check .",
        "pretty": "npx pretty-quick .",
        "build-pretty": "npm run build && npm run pretty",
        "pack": "./node_modules/.bin/webpack -p",
        "set-up-hooks": "cp hooks/pre-commit .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit",
        "lint": "cd lib && npx tslint -c tslint.json './**/*.ts'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/supertokens/supertokens-web-js.git"
    },
    "keywords": [
        "auth",
        "authentication",
        "authorisation",
        "supertokens",
        "chrome",
        "firefox",
        "IE",
        "safari"
    ],
    "contributors": [],
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/supertokens/supertokens-web-js/issues"
    },
    "homepage": "https://github.com/supertokens/supertokens-web-js#readme",
    "dependencies": {
        "supertokens-js-override": "0.0.4",
        "supertokens-website": "github:supertokens/supertokens-website#add-post-api-hook"
    },
    "devDependencies": {
        "@babel/core": "^7.15.5",
        "@babel/plugin-transform-runtime": "^7.15.0",
        "@babel/preset-env": "^7.15.6",
        "@size-limit/preset-small-lib": "^6.0.3",
        "madge": "^5.0.1",
        "mocha": "^9.1.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.5.1",
        "pretty-quick": "^3.1.3",
        "size-limit": "^6.0.3",
        "tslint": "^6.1.3",
        "typescript": "^4.5.4",
        "webpack": "4.35.0",
        "webpack-cli": "3.3.5",
        "webpack-dev-server": "3.7.2"
    },
    "size-limit": [
        {
            "path": "lib/build/index.js",
            "limit": "5kb"
        },
        {
            "path": "recipe/emailverification/index.js",
            "limit": "6kb"
        },
        {
            "path": "recipe/emailpassword/index.js",
            "limit": "15kb"
        },
        {
            "path": "recipe/thirdparty/index.js",
            "limit": "16kb"
        },
        {
            "path": "recipe/thirdpartyemailpassword/index.js",
            "limit": "17kb"
        },
        {
            "path": "recipe/session/index.js",
            "limit": "13kb"
        }
    ]
}
